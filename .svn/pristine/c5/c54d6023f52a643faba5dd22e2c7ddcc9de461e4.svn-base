.well {
    position: relative;
    min-height: 20px;
    padding: 0px;
    margin-bottom: 20px;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    border-radius: 0;
}

.well blockquote {
    border-color: #ddd;
    border-color: rgba(0, 0, 0, 0.15);
}

.well-lg {
    padding: 24px;
}

.well-sm {
    padding: 0;
}

.well-header {
    min-height: 40px;
    background: #0072c6;
    padding: 0;
}

.well.blue .well-header {
    background: #0072c6;
}

.well.blue .well-content {
    border-top: 1px solid #0072c6;
    border-left: 1px solid #0072c6;
    border-right: 1px solid #0072c6;
    border-bottom: 1px solid #0072c6;
}

